<form class="form-inline" action="/register" method="POST">
  <div class="form-group">
  <label for="preferences">Any specific languages you are looking for?</label>  <br>
  <select name="language" class="preference">
    <option value= "">
    <option value= >
    <% @languages.each do |language| %>
    <%= language.name %>
    <option value= <%= language.name %>>
      <% end %>
    </select><br><br>
  </div>

  <div class="form-group">
  <label for="preferences">Genre</label>  <br>
  <select name="genre" class="preference">
    <option value= "">
    <option value= >
    <% @languages.each do |language| %>
    <%= language.name %>
    <option value= <%= language.name %>>
      <% end %>
    </select><br><br>
  </div>

  <button type="submit" class="btn btn-default">Register</button>
</form>
